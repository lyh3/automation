    <script type="text/javascript">
        String.prototype.format = function () {
            var a = this;
            for (var k in arguments) {
                a = a.replace(new RegExp("\\{" + k + "\\}", 'g'), arguments[k]);
            }
            return a
        }
        $(document).ready(function () {
            displayLoader(false);
            
            if (document.baseURI.endsWith('MicrocodeRelease')) {
                updateMicrocodReleaeUI(document.baseURI.endsWith('MicrocodeRelease'));
            } else if (document.baseURI.endsWith('RemoteSystemReset')) {
                updateSystemResetUI();
            } else if (document.baseURI.endsWith('ReleaseOchestration')) {
                updateTransactionStatus();
            } else {
                updateBMCStatus();
            }
            $('#divCommandDoc').attr("style", "display:none;");
        });
        function rgb2hex(red, green, blue) {
            var rgb = blue | (green << 8) | (red << 16);
            return '#' + (0x1000000 + rgb).toString(16).slice(1)
        }

        function hex2rgb(hex) {
            // long version
            r = hex.match(/^#([0-9a-f]{2})([0-9a-f]{2})([0-9a-f]{2})$/i);
            if (r) {
                return r.slice(1,4).map(function(x) { return parseInt(x, 16); });
            }
            // short version
            r = hex.match(/^#([0-9a-f])([0-9a-f])([0-9a-f])$/i);
            if (r) {
                return r.slice(1,4).map(function(x) { return 0x11 * parseInt(x, 16); });
            }
            return null;
        }

        function rgb2hex(red, green, blue) {
            var rgb = blue | (green << 8) | (red << 16);
            return '#' + (0x1000000 + rgb).toString(16).slice(1)
        }

        function hex2rgb(hex) {
            // long version
            r = hex.match(/^#([0-9a-f]{2})([0-9a-f]{2})([0-9a-f]{2})$/i);
            if (r) {
                return r.slice(1,4).map(function(x) { return parseInt(x, 16); });
            }
            // short version
            r = hex.match(/^#([0-9a-f])([0-9a-f])([0-9a-f])$/i);
            if (r) {
                return r.slice(1,4).map(function(x) { return 0x11 * parseInt(x, 16); });
            }
            return null;
        }
        $('#imgTools').on('mouseover', function () {
            var toolsWindow = $("#ToolsSelectWindow").data("kendoWindow");
            toolsWindow.open()
            $("#ToolsSelectWindow").closest(".k-window").css({
                top: 50,
                right: 5,
            });
        })
        function IsFormFilled() {
            var isFormFilled = true;
            var inputs = document.getElementsByTagName('input');
            for (var i = 0; i < inputs.length; i++) {
                if (inputs[i].hasAttribute('required')) {
                    if (inputs[i].value == "") {
                        isFormFilled = false;
                        break;
                    }
                }
            }
            return isFormFilled;
        }
    </script>
